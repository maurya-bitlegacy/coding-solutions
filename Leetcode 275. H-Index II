class Solution:
    def hIndex(self, citations: List[int]) -> int:
        if not citations or citations[-1]==0:
            return 0
        low=0
        high=len(citations)-1
        while(low<high):
            mid = (low+high)//2
            papers = len(citations)-mid
            if citations[mid]>=papers:
                high = mid
            else:
                low=mid+1
        return len(citations)-low
            
